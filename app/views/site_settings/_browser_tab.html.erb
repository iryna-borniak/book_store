<div class="tab-browser">
  <div class="tab-content">
    <%= image_tag(SiteSetting.current.favicon, class: "tab-icon", alt: "Tab Browser Image", data: { handle_browser_tab_target: "icon" }) %>
    <span class="tab-text" data-handle-browser-tab-target="title">
      <%= SiteSetting.current.title %>
    </span>
  </div>
</div>
